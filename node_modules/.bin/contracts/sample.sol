pragma solidity >= 0.5.0 < 0.7.0;
contract vehicle{
 
struct upload{

	// State variables
	uint upload_id;
	uint vehicle_id;
	string ty;
	string file;
	
	

}

 
upload []m;



function add_upload(uint upload_id,uint vehicle_id,string memory ty,string memory file) public{
	upload memory m1=upload(upload_id,vehicle_id,ty,file);
	m.push(m1);
}

struct vehicles{

	// State variables
	uint vehicle_id;
	uint user_id;
	string veh;
	string detail;
	string image;
	
	

}

 
vehicles []v;



function add_vehicle(uint vehicle_id,uint user_id,string memory veh,string memory detail,string memory image) public{
	vehicles memory v1=vehicles(vehicle_id,user_id,veh,detail,image);
	v.push(v1);
}





}




